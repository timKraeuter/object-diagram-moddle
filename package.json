{
  "name": "object-diagram-moddle",
  "version": "1.0.2",
  "description": "A moddle wrapper for BPMN 2.0",
  "scripts": {
    "all": "run-s lint test distro",
    "lint": "eslint .",
    "fixLint": "eslint . --fix",
    "dev": "npm test -- --watch",
    "test": "mocha --reporter=spec --recursive",
    "distro": "run-s build test:build",
    "build": "rollup -c",
    "test:build": "mocha --reporter=spec --recursive test/distro",
    "prepare": "run-s distro"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/timKraeuter/object-diagram-moddle"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "umd:main": "dist/object-diagram-moddle.umd.cjs",
  "source": "lib/index.js",
  "keywords": [
    "object diagram",
    "moddle",
    "meta-model"
  ],
  "author": {
    "name": "Tim Kr√§uter",
    "url": "https://timkraeuter.com/"
  },
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-import-assertions": "^7.18.6",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "acorn-import-assertions": "^1.8.0",
    "bpmn-in-color-moddle": "^0.1.0",
    "chai": "^4.3.10",
    "eslint": "^8.24.0",
    "eslint-plugin-bpmn-io": "^0.16.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.29.5",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "min-dash": "^4.1.1",
    "moddle": "^6.2.3",
    "moddle-xml": "^10.1.0"
  },
  "files": [
    "dist",
    "resources",
    "!resources/bpmn/cmof"
  ]
}
